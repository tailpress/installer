{
  "name": "tailpress/installer",
  "description": "TailPress installer package.",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeffrey van Rossum",
      "email": "jeffrey@vanrossum.dev"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.0.2",
    "symfony/console": "^6.0|^7.0",
    "symfony/process":  "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3",
    "spatie/ray": "dev-main"
  },
  "bin": [
    "bin/tailpress"
  ],
  "autoload": {
    "psr-4": {
      "TailPress\\Installer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TailPress\\Installer\\Console\\Tests\\": "tests/"
    }
  }
}
